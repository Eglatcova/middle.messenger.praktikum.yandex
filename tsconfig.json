{
  "compilerOptions": {
    "outDir": "dist",
    "lib": ["DOM", "ES2015", "ESNext"],
    "target": "ES6",
    "declaration": false,
    "module": "commonjs",
    "strictNullChecks": true,
    "sourceMap": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "noUnusedLocals": true
  }
}
