.settings-input_wrapper {
  width: 100%;
  display: flex;
  justify-content: space-between;
  padding: 10px 0;
  border-bottom: 1px solid var(--greyLight);
}

.settings-input_wrapper:last-child {
  border-bottom: none;
}

.settings-input_input {
  border: none;
  outline: none;
  color: var(--grey);
}
